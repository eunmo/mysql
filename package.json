    "lint": "eslint --fix src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eunmo/mysql.git"
  },
  "author": "Eunmo Yang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eunmo/mysql/issues"
  },
  "homepage": "https://github.com/eunmo/mysql#readme",
  "dependencies": {
    "config": "^3.3.1",
    "mysql": "^2.18.1"
